{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "types": [
      "@dcloudio/types",
      "@uni-helper/uni-app-types"
    ]
  },
  "include": [
    "**/*.ts",
    "**/*.d.ts",
    "**/*.vue",
    "**/*.tsx"
  ],
  "exclude": [
    "**/dist/**",
    "**/locale/**"
  ],
  "vueCompilerOptions": {
    "experimentalRuntimeMode": "runtime-uni-app",
    "nativeTags": [
      "block",
      "component",
      "template",
      "slot"
    ]
  }
}
