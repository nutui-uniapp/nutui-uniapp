{
  "name": "nutui-uniapp",
  "displayName": "nutui-uniapp 京东风格的轻量级移动端 Uniapp、Vue3 组件库",
  "version": "1.7.6",
  "description": "京东风格的轻量级移动端 Uniapp、Vue3 组件库（支持小程序开发）",
  "author": "yang1206 <y1149221897@outlook.com>",
  "license": "MIT",
  "homepage": "https://github.com/nutui-uniapp/nutui-uniapp#readme",
  "repository": "https://github.com/nutui-uniapp/nutui-uniapp.git",
  "bugs": "https://github.com/nutui-uniapp/nutui-uniapp/issues",
  "keywords": [
    "nutui",
    "小程序",
    "vue3",
    "暗黑模式",
    "mini-program",
    "vue3 component"
  ],
  "sideEffects": [
    "*.scss",
    "*/components/**/*.vue"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./*": "./*",
    "./composables": "./components/composables.ts",
    "./locale": "./locale/index.ts",
    "./package.json": "./package.json"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "web-types": "./web-types.json",
  "vetur": {
    "tags": "./tags.json",
    "attributes": "./attributes.json"
  },
  "files": [
    "README.md",
    "attributes.json",
    "components",
    "components/composables.ts",
    "dist",
    "global.d.ts",
    "locale/**",
    "styles",
    "tags.json",
    "web-types.json"
  ],
  "scripts": {
    "build": "unbuild",
    "stub": "unbuild --stub",
    "type-check": "vue-tsc --noEmit"
  },
  "peerDependencies": {
    "@uni-helper/uni-app-types": ">=0.5.9",
    "vue": ">=3.2.0"
  }
}
